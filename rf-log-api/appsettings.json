{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    },
    "File": {
      "Path": "logs/app.log",
      "Append": true,
      "MaxFileSize": 10485760,
      "RollingInterval": "Day",
      "RollingStyle": "SizeAndAge"
    }
  }
}